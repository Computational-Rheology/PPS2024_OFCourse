localRefinement
{
}


renameBoundary
{
    //defaultName        ;
    defaultType        patch;

    newPatchNames
    {
        wall
        {
            newName     wall;
            type     wall;
        }
        
        in
        {
            newName     in;
            type     patch;
        }

        out
        {
            newName     out;
            type     patch;
        }

    }
}

